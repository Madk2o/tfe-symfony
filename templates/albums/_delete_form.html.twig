<form method="post" action="{{ path('albums_delete', {'albumid': album.albumid}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ album.albumid) }}">
    <button class="btn btn-danger m-1">Effacer</button>
</form>
